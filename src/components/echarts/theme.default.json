{
    "echarts": {
        "color": [
            "#2288FC", "#00C79A", "#AFD527", "#FDAB2B", "#FF786D", "#A26FEA", "#49A9C5",
            "#818B43", "#FF6B02", "#FE4C32", "#5E6E86", "#3669B6", "#007FFF", "#DD4C9A",
            "#FA5CB0", "#FF44DF", "#955DFF", "#346FF9", "#0BC9F3", "#19B089", "#FEBB64"
        ],
        "title": {
            "textStyle": {
                "fontWeight": 800,
                "fontSize": 18,
                "color": "#333333"
            }
        },
        "legend": {
            "textStyle": {
                "fontWeight": 400,
                "fontSize": 12,
                "color": "#666666"
            },
            "pageTextStyle": {
                "fontWeight": 400,
                "fontSize": 12,
                "color": "#999999"
            },
            "pageIconColor": "#2288FC",
            "pageIconInactiveColor": "#999999"
        },
        "categoryAxis": {
            "axisLabel": {
                "textStyle": {
                    "fontWeight": 400,
                    "fontSize": 12,
                    "color": "#999999"
                }
            },
            "axisLine": {
                "lineStyle": {
                    "width": 1,
                    "type": "solid",
                    "color": "#E5E5E5"
                }
            },
            "splitLine": {
                "lineStyle": {
                    "width": 1,
                    "type": "dashed",
                    "color": "#E5E5E5"
                }
            }
        },
        "valueAxis": {
            "axisLabel": {
                "textStyle": {
                    "fontWeight": 400,
                    "fontSize": 12,
                    "color": "#999999"
                }
            },
            "axisLine": {
                "lineStyle": {
                    "width": 1,
                    "type": "solid",
                    "color": "#E5E5E5"
                }
            },
            "splitLine": {
                "lineStyle": {
                    "width": 1,
                    "type": "dashed",
                    "color": "#E5E5E5"
                }
            }
        }
    },
    "echartsFunnel": {
        "series": [
            {
                "label": {
                    "normal": {
                        "fontWeight": 400,
                        "fontSize": 12,
                        "color": "#999999"
                    }
                },
                "labelLine": {
                    "normal": {
                        "lineStyle": {
                            "width": 1,
                            "type": "solid",
                            "color": "#999999"
                        }
                    }
                }
            }
        ]
    },
    "echartsHeatmap": {
        "xAxis": {
            "splitArea": {
                "show": true,
                "areaStyle": {
                    "color": [
                        "rgba(85,164,254,0.06)",
                        "rgba(34,136,252,0.09)"
                    ]
                }
            }
        },
        "yAxis": {
            "splitArea": {
                "show": true,
                "areaStyle": {
                    "color": [
                        "rgba(0,0,0,0)",
                        "rgba(34,136,252,0.09)"
                    ]
                }
            }
        },
        "visualMap": {
            "color": [
                "#2288FC",
                "#E7F2FF"
            ],
            "textStyle": {
                "fontWeight": 400,
                "fontSize": 12,
                "color": "#999999"
            }
        },
        "series": [
            {
                "label": {
                    "normal": {
                        "show": true,
                        "fontWeight": 400,
                        "fontSize": 12,
                        "color": "#FFFFFF"
                    }
                }
            }
        ]
    },
    "echartsValdistmap": {
        "visualMap": {
            "color": [
                "#2388FD",
                "#6BB0FD"
            ]
        },
        "series": [
            {
                "itemStyle": {
                    "emphasis": {
                        "color": "#FDAB2B"
                    }
                }
            },
            {
                "itemStyle": {
                    "normal": {
                        "areaColor": "#E5E5E5",
                        "borderColor": "#FFFFFF"
                    },
                    "emphasis": {
                        "areaColor": "#FDAB2B",
                        "borderColor": "#FFFFFF"
                    }
                }
            }
        ]
    }
}